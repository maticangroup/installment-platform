        .category-selector h2 {
            font-size: 25px !important;
            font-weight: bold;
        }

        .category-selector ul {

        }

        .category-selector ul li {
            font-size: 23px;
        }

        .category-selector p {
            color: darkblue !important;
        }

        .card p {
            color: black !important;
        }

        .card {
            position: relative;
        }

        .sticky-button {
            position: absolute;
            left: 50%;
            transform: translate(-50%, 0);
            bottom: 15px;
        }

        .btn {
            font-size: 20px !important;
        }
    </style>
    {% if(app.session.flashBag.peek('f') or app.session.flashBag.peek('s')) %}
        <div class="row mt-5">
            <div class="col-12 text-center">
                <div class="alert alert-danger alert-dismissible fade {{ (app.session.flashBag.peek('f')) ? 'show' : '' }}"
                     role="alert">
                    <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                        <span aria-hidden="true">×</span>
                    </button>
                    {% for message in app.session.flashBag.get('f') %}
                        {{ message }}
                    {% endfor %}
                </div>
                <div class="alert alert-success alert-dismissible fade {{ (app.session.flashBag.peek('s')) ? 'show' : '' }}"
                     role="alert">
                    <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                        <span aria-hidden="true">×</span>
                    </button>
                    {% for message in app.session.flashBag.get('s') %}
