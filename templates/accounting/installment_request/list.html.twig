                                                                                                        <button type="button"
                                                                                                                class="btn btn-secondary btn-sm rounded-circle mo-mb-2"
                                                                                                                data-toggle="tooltip"
                                                                                                                data-placement="top"
                                                                                                                title=""
                                                                                                                data-original-title="{{ 'accounting.installment.user.personal.information.salary.account.number.tooltip' | trans }}">
                                                                                                            ?
                                                                                                        </button>
                                                                                                        <h4>{{ installmentPayment.investAccountNumber }}</h4>
                                                                                                    </div>
                                                                                                    <div class="form-group m-b-0">
                                                                                                        <label>{{ 'accounting.installment.user.personal.information.create.account.year.title' | trans }}</label>
                                                                                                        <h4>{{ installmentPayment.accountCreatedDate }}</h4>
                                                                                                    </div>
                                                                                                </div>
                                                                                            </div>
                                                                                            <div class="col-lg-6">
                                                                                                <div class="p-20">
                                                                                                    <div class="form-group">
                                                                                                        <label>{{ 'general.cheque.type.title' | trans }}</label>
                                                                                                        <h4>{{ ('accounting.installment.form.cheque.type.drop.down.' ~ installmentPayment.chequeType) | trans }}</h4>
                                                                                                    </div>
                                                                                                    <div class="form-group">
                                                                                                        <label>{{ 'accounting.installment.user.personal.information.bank.code.branch.name.title' | trans }}</label>
                                                                                                        <h4>{{ installmentPayment.branchName }}</h4>
                                                                                                    </div>
                                                                                                    <div class="form-group">
                                                                                                        <label>{{ 'accounting.installment.user.personal.information.product.type.and.description.title' | trans }}</label>
                                                                                                        <h4>{{ installmentPayment.productName }}</h4>
                                                                                                    </div>
                                                                                                    <div class="form-group">
                                                                                                        <label>{{ 'general.buy.price.title' | trans }}</label>
                                                                                                        <h4>{{ installmentPayment.productPrice }}</h4>
                                                                                                    </div>
                                                                                                </div>
                                                                                            </div> <!-- end col -->
                                                                                        </div> <!-- end row -->
                                                                                    </div>
                                                                                </div>
                                                                            </div> <!-- end col -->
                                                                        </div>
                                                                    </div>
                                                                </div><!-- /.modal-content -->
                                                            </div><!-- /.modal-dialog -->
                                                        </div>

                                                    </td>
                                                {% endif %}
                                            </tr>
                                        {% endfor %}
                                        </tbody>
                                    </table>
                                </div>

                            </div>

                        </div>
                    </div>
                </div>
            </div>
            {#Installment Payments Table - Admin#}

        {% endif %}

    {% else %}
        {#First Customer Login - Customer (Personal Information)#}
        <div class="page-title-box">
            <div class="row align-items-center">
                <div class="col-sm-6">
                    <h4 class="page-title">{{ 'general.basic.information.title' | trans }}</h4>
                </div>
            </div>
        </div>
        {#New Form#}
        <form action="{{ path('accounting_installment_request_personal_info') }}" method="post">
            <div class="row">
                <div class="col-12">
                    <div class="card m-b-30">
                        <div class="card-body">
                            <h4 class="mt-0 header-title">{{ 'accounting.installment.user.personal.information.title' | trans }}</h4>
                            <p class="sub-title">{{ 'accounting.installment.user.personal.information.description' | trans }}</p>
                            <div class="row">
                                <div class="col-lg-6">
                                    <div class="p-20">
                                        <div class="form-group">
                                            <label>{{ 'general.gender.title' | trans }}</label>
                                            <select class="form-control" required
                                                    name="genderStatus">
                                                {#<option value="none">{{ 'general.select.title' | trans }}</option>#}
                                                {% for gender in genders %}
                                                    <option value="{{ gender.machineName }}">{{ ('accounting.installment.form.gender.drop.down.' ~ gender.machineName) | trans }}</option>
                                                {% endfor %}
                                            </select>
                                        </div>
                                        <div class="form-group">
                                            <label>{{ 'general.national.code.title' | trans }}</label>
                                            <input type="text"
                                                   class="form-control" name="nationalCode"
                                                   value="{{ personalInformation.nationalCode }}" required>
                                        </div>
                                        <div class="form-group">
                                            <label>{{ 'general.first.name.title' | trans }}</label>
                                            <input type="text"
                                                   class="form-control" name="firstName"
                                                   value="{{ personalInformation.firstName }}" required>
                                        </div>
                                        <div class="form-group">
                                            <label>{{ 'general.last.name.title' | trans }}</label>
                                            <input type="text" class="form-control"
                                                   name="lastName" value="{{ personalInformation.lastName }}" required>
                                        </div>
                                        <div class="form-group m-b-0">
                                            <label>{{ 'general.father.name.title' | trans }}</label>
                                            <input type="text" class="form-control"
                                                   name="fatherName" value="{{ personalInformation.fatherName }}"
                                                   required>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-lg-6">
                                    <div class="p-20">
                                        <div class="form-group">
                                            <label>{{ 'general.marriage.status.title' | trans }}</label>
                                            <select class="form-control" name="marriageStatus" required>
                                                {#<option value="none">{{ 'general.select.title' | trans }}</option>#}
                                                {% for marriage in marriages %}
                                                    <option value="{{ marriage.machineName }}">{{ ('accounting.installment.form.marriage.status.drop.down.' ~ marriage.machineName) | trans }}</option>
                                                {% endfor %}
                                            </select>
                                        </div>

                                        <div class="form-group">
                                            <label>{{ 'general.birth.date.title' | trans }}</label>

                                            <input type="text" class="form-control" name="birthDate"
                                                   id="date-picker-date-id"
                                                   value="{{ personalInformation.birthDate }}" required>
                                            <span id="span-date-picker"></span>
                                        </div>
                                        <div class="form-group">
                                            <label>{{ 'general.birth.certificate.code.title' | trans }}</label>
                                            <input type="text" class="form-control" name="birthCertificateCode"
                                                   value="{{ personalInformation.birthCertificateCode }}" required>
                                        </div>
                                        <div class="form-group">
                                            <label>{{ 'general.email.title' | trans }}</label>
                                            <input type="email" class="form-control" name="email"
                                                   value="{{ personalInformation.email }}" required>
                                        </div>
                                    </div>
                                </div> <!-- end col -->
                            </div> <!-- end row -->
                        </div>
                    </div>
                </div>
                <div class="col-12">
                    <div class="card m-b-30">
                        <div class="card-body">
                            <h4 class="mt-0 header-title">{{ 'accounting.installment.user.personal.information.job.address.title' | trans }}</h4>
                            <p class="sub-title">{{ 'accounting.installment.user.personal.information.description' | trans }}</p>
                            <div class="row">
                                <div class="col-lg-6">
                                    <div class="p-20">
                                        <div class="form-group">
                                            <label>{{ 'general.job.title' | trans }}</label>
                                            <button type="button"
                                                    class="btn btn-secondary btn-sm rounded-circle mo-mb-2"
                                                    data-toggle="tooltip" data-placement="top" title=""
                                                    data-original-title="{{ 'accounting.installment.user.personal.information.job.tooltip' | trans }}">
                                                ?
                                            </button>
                                            <input type="text"
                                                   class="form-control" name="job"
                                                   value="{{ personalInformation.job }}" required>
                                        </div>
                                        <div class="form-group">
                                            <label>{{ 'general.company.name.or.job.place.title' | trans }}</label>
                                            <input type="text"
                                                   class="form-control" name="companyName"
                                                   value="{{ personalInformation.companyName }}" required>
                                        </div>
                                        <div class="form-group">
                                            <label>{{ 'general.earn.monthly.title' | trans }}</label>
                                            <input type="text"
                                                   class="form-control" name="monthlySalary"
                                                   value="{{ personalInformation.monthlySalary }}" required>
                                        </div>
                                        <div class="form-group">
                                            <label>{{ 'general.job.status.title' | trans }}</label>
                                            <select class="form-control" name="jobStatus" required>
                                                {#<option value="none">{{ 'general.select.title' | trans }}</option>#}
                                                {% for jobStatus in jobStatuses %}
                                                    <option value="{{ jobStatus.machineName }}">{{ ('accounting.installment.form.job.status.drop.down.' ~ jobStatus.machineName) |trans }}</option>
                                                {% endfor %}
                                            </select>
                                        </div>
                                        <div class="form-group m-b-0">
                                            <label>{{ 'general.job.address.title' | trans }}</label>
                                            <input type="text" class="form-control" name="jobAddress"
                                                   value="{{ personalInformation.jobAddress }}" required>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-lg-6">
                                    <div class="p-20">
                                        <div class="form-group">
                                            <label>{{ 'general.job.phone.title' | trans }}</label>
                                            <input type="text" class="form-control" name="jobPhoneNumber"
                                                   value="{{ personalInformation.jobPhoneNumber }}" required>
                                        </div>
                                        <div class="form-group">
                                            <label>{{ 'general.province.title' | trans }}</label>
                                            <select class="form-control" name="province" id="province"
                                                    required></select>
                                        </div>
                                        <div class="form-group">
                                            <label>{{ 'general.city.title' | trans }}</label>
                                            <select class="form-control" name="city" id="city" required></select>
                                        </div>
                                        <div class="form-group">
                                            <label>{{ 'general.home.address.title' | trans }}</label>
                                            <input type="text" class="form-control" name="homeAddress"
                                                   value="{{ personalInformation.homeAddress }}" required>
                                        </div>
                                        <div class="form-group m-b-0">
                                            <label>{{ 'general.home.phone.title' | trans }}</label>
                                            <input type="text" class="form-control" name="homePhoneNumber"
                                                   value="{{ personalInformation.homePhoneNumber }}" required>
                                        </div>
                                    </div>
                                </div> <!-- end col -->
                            </div> <!-- end row -->
                        </div>
                    </div>
                </div>
                <div class="col-4"></div>
                <div class="col-4">
                    <button type="submit"
                            class="btn btn-primary btn-lg btn-block waves-effect waves-light mb-4">{{ 'general.send.title' | trans }}
                    </button>
                </div>
                <div class="col-4"></div>
            </div>
        </form>
        {#New Form#}
        {#First Customer Login - Customer (Personal Information)#}
    {% endif %}
{% endblock %}
