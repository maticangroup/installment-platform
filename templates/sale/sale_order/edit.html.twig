                                        <th scope="col">{{ 'general.total.price.title' | trans }}</th>
                                    </tr>
                                    </thead>
                                    <tbody>

                                    {% for selectedProduct in selectedProducts %}
                                        <input type="hidden" name="productId[]"
                                               value="{{ selectedProduct.productId }}">
                                        <input type="hidden" name="productOrderItemId[]"
                                               value="{{ selectedProduct.productOrderItemId }}">
                                        {% if selectedProduct.productTypeMachineName == 'unique' %}
                                            <tr>

                                                <td>{{ selectedProduct.productItemName }}</td>
                                                <td>{{ selectedProduct.productGuaranteeName }}</td>
                                                <td>
                                                    {{ selectedProduct.productColorName }}
                                                    <span class="label rounded"
                                                          style="background-color: {{ selectedProduct.productColorHex }};">  </span>
                                                </td>
                                                <td>{{ selectedProduct.productSupplierName }}</td>
                                                <td><input type="text" class="form-control"
                                                           value="{{ selectedProduct.productSerial }}"
                                                           name="productSerial[]" required>
                                                </td>
                                                <td>
                                                    <input type="hidden" name="productQuantity[]"
                                                           value="{{ selectedProduct.productQuantity }}">
                                                    {{ selectedProduct.productQuantity }}
                                                </td>
                                                <td>{{ selectedProduct.productCurrentPlace }}</td>
                                                <td>{{ selectedProduct.productCurrentPrice }}</td>
                                                <td>{{ selectedProduct.productDiscountPrice }}</td>
                                                <td>{{ selectedProduct.productTotalPrice }}</td>
                                            </tr>
                                        {% else %}
                                            <tr>
                                                <td>{{ selectedProduct.productItemName }}</td>
                                                <td>{{ selectedProduct.productGuaranteeName }}</td>
                                                <td>
                                                    {{ selectedProduct.productColorName }}
                                                    <span class="label rounded"
                                                          style="background-color: {{ selectedProduct.productColorHex }};">  </span>
                                                </td>
                                                <td>{{ selectedProduct.productSupplierName }}</td>
                                                <td>
                                                    <input type="hidden" name="productSerial[]"
                                                           value="{{ selectedProduct.productSerial }}">
                                                    {{ selectedProduct.productSerial }}
                                                </td>
                                                <td>
                                                    <input type="number" class="form-control"
                                                           value="{{ selectedProduct.productQuantity }}"
                                                           name="productQuantity[]" required min="1"
                                                           max="{{ selectedProduct.productMaxQuantity }}">
                                                </td>
                                                <td>{{ selectedProduct.productCurrentPlace }}</td>
                                                <td>{{ selectedProduct.productCurrentPrice }}</td>
                                                <td>{{ selectedProduct.productDiscountPrice }}</td>
                                                <td>{{ selectedProduct.productTotalPrice }}</td>
                                            </tr>
                                        {% endif %}
                                    {% endfor %}

                                    <tr>
                                        <td colspan="8"><strong>{{ 'general.vat.title' | trans }}</strong></td>
                                        <td>
                                            {% if orderModel.orderVatPrice %}
                                                {{ orderModel.orderVatPrice }}
                                            {% endif %}
                                        </td>
                                    </tr>
                                    <tr>
                                        <td colspan="8"><strong>{{ 'general.total.price.title' | trans }}</strong></td>
                                        <td>
                                            {% if orderModel.orderTotalPrice %}
                                                <strong>{{ orderModel.orderTotalPrice }}</strong>
                                            {% endif %}
                                        </td>
                                    </tr>
                                    </tbody>
                                </table>
                            </div>
                            <div class="invoice--footer">
                                <ul class="list-unstyled">
                                    <li class="mb-3"><strong>{{ 'general.description.title' | trans }}</strong>
                                        <ul class="list-unstyled">
                                            <li>
                                                {{ orderModel.orderDescription }}
                                            </li>
                                        </ul>
                                    </li>
                                </ul>
                            </div>
                            {% if canConfirm %}
                                <div class="row">
                                    <div class="col-4"></div>
                                    <div class="col-4">
                                        <div class="panel">
                                            <div class="weather--panel text-white bg-darker">
                                                <div class="weather--title">
                                                    <i class="fa fa-exclamation"></i>
                                                    <span>{{ 'general.final.confirm.title' | trans }}</span>
                                                </div>

                                                <div class="weather--info">
                                                    <a href="{{ path('sale_order_confirm' , {order_id:orderModel.orderId}) }}"
                                                       class="fa fa-check text-green"></a>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-4"></div>
                                </div>
                            {% endif %}
                        </form>
                    </div>
                </div>
            </div>
        {% endif %}
